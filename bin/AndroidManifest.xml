<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelexpensetracker"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.travelexpensetracker.MainClaimActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.travelexpensetracker.ClaimListActivity"
            android:label="@string/title_activity_claim_list" >
        </activity>
        <activity
            android:name="com.example.travelexpensetracker.CreateClaimActivity"
            android:label="@string/title_activity_create_claim" >
        </activity>
        <activity
            android:name="com.example.travelexpensetracker.EditDeleteClaimActivity"
            android:label="@string/title_activity_edit_delete_claim" >
        </activity>
        <activity
            android:name="com.example.travelexpensetracker.SubmitReturnClaimActivity"
            android:label="@string/title_activity_submit_return_claim" >
        </activity>
        <activity
            android:name="com.example.travelexpensetracker.ApproveClaimActivity"
            android:label="@string/title_activity_approve_claim" >
        </activity>
    </application>

</manifest>
